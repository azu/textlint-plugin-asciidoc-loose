{
  "name": "textlint-plugin-asciidoc-loose",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azu/textlint-plugin-asciidoc-loose.git"
  },
  "author": "azu",
  "email": "azuciao@gmail.com",
  "homepage": "https://github.com/azu/textlint-plugin-asciidoc-loose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azu/textlint-plugin-asciidoc-loose/issues"
  },
  "version": "1.0.0",
  "description": "textlint plugin for asciidoc/asciidoctor",
  "main": "lib/textlint-plugin-asciidoc-loose.js",
  "files": [
    "src/",
    "lib/",
    "syntax/"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present build",
    "test": "mocha"
  },
  "keywords": [
    "textlint",
    "plugin",
    "asciidoc"
  ],
  "dependencies": {
    "structured-source": "^3.0.2",
    "vscode-textmate": "^1.0.11"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.8.0",
    "mocha": "^2.4.5",
    "power-assert": "^1.4.0",
    "textlint": "^6.6.0",
    "textlint-ast-test": "^1.1.3",
    "textlint-rule-no-todo": "^2.0.0"
  }
}
